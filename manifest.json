{
  "manifest_version": 3,
  "name": "LinkedIn easy apply follow company un-checker",
  "version": "1.0",
  "description": "This extension modifies the LinkedIn Follow Company checkbox to be checked by default.",
  "permissions": ["scripting", "activeTab", "storage"],
  "host_permissions": ["https://www.linkedin.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
